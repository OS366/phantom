{
  "name": "phantom",
  "version": "0.1.4-beta",
  "description": "Lightweight helper library for OIE scripting",
  "main": "phantom.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "minify": "node minify.js"
  },
  "keywords": [
    "oie",
    "oracle",
    "integration"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "jest": "^29.7.0",
    "terser": "^5.44.1",
    "uglify-js": "^3.19.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "phantom.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      "<rootDir>/test-summary-reporter.js"
    ]
  }
}
