{
  "name": "phantom",
  "version": "0.1.1",
  "description": "Lightweight helper library for OIE scripting",
  "main": "phantom.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "minify": "terser phantom.js -o phantom.min.js -c -m --comments false"
  },
  "keywords": [
    "oie",
    "oracle",
    "integration"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "jest": "^29.7.0",
    "terser": "^5.44.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "phantom.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
