{
  "name": "phantom",
  "version": "0.1.6-beta",
  "description": "Lightweight helper library for OIE scripting",
  "main": "phantom.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:check": "node check-test-coverage.js --strict",
    "test:check-diff": "node check-test-coverage.js --strict --diff",
    "minify": "node minify.js",
    "release": "node create-release.js",
    "release:prepare": "node prepare-release.js"
  },
  "keywords": [
    "oie",
    "oracle",
    "integration"
  ],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "jest": "^29.7.0",
    "terser": "^5.44.1",
    "uglify-js": "^3.19.3"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "..",
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "phantom.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/scripts/"
    ],
    "reporters": [
      "default",
      "<rootDir>/test-summary-reporter.js"
    ]
  }
}
